<script>
  $('#average-rating').raty({
    readOnly: true,
    score: <%= @market.rating %>,
    path: '/assets'
  });

  $('#user-rating').raty({
    score: <%= @rating.score %>,
    path: '/assets',
    click: function(score, evt) {
      $.ajax({
        url: '/m/' + <%= @market.id %> + '/ratings/' + <%= @rating.id %>,
        type: 'PATCH',
        data: { score: score }
      }).done(function (data){});
    }
  });
</script>
