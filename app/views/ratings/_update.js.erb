<script>
  $('#average-rating').raty({
    readOnly: true,
    score: <%= 5 %>,
    path: '/assets'
  });

  $('#user-rating').raty({
    score: <%= @rating.score %>,
    path: '/assets',
    click: function(score, evt) {
      $.ajax({
        url: '/ratings/' + 1,
        type: 'PATCH',
        data: { score: score }
      }).done(function (data){});
    }
  });
</script>
