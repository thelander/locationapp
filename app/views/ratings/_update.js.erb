<script>
  $("#rating").raty({
    score: <%= @market.rating %>,
    path: "/assets",
    click: function(score, evt) {
      $.ajax({
        url: "<%= market_rating_path(@market, @rating) %>",
        type: "PATCH",
        data: { score: score }
      }).done(function (data){});
    }
  });
</script>
