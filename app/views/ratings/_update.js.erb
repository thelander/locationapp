<script>
  $("#rating").raty({
    score: <%= @market.rating %>,
    path: "..",
    starOn: "<%= asset_path('star-on.png') %>",
    starOff: "<%= asset_path('star-off.png') %>",
    click: function(score, evt) {
      $.ajax({
        url: "<%= market_rating_path(@market, @rating) %>",
        type: "PATCH",
        data: { score: score }
      }).done(function (data){});
    }
  });
</script>

