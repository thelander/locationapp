<script>
  $("#average-rating").raty({
    readOnly: true,
    score: <%= @market.rating %>,
    path: "/assets"
  });

  $("#user-rating").raty({
    score: <%= @rating.score %>,
    path: "/assets",
    click: function(score, evt) {
      $.ajax({
        url: "<%= market_rating_path(@market, @rating) %>",
        type: "PATCH",
        data: { score: score }
      }).done(function (data){});
    }
  });
</script>
